{
  "name": "myordbok",
  "version": "1.6.5",
  "description": "online Myanmar dictionary",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./webpack.production.js",
    "test": "webpack-dev-server --open --config webpack.development.js",
    "update-package": "node update-package",
    "update-repository": "node update-repository",
    "dev-local": "node serve",
    "test-port": "set PORT=5000 && node serve",
    "test-listen": "set LISTEN= && node serve",
    "test-production": "set NODE_ENV=production && node serve",
    "pm2:kill": "pm2 kill",
    "pm2:startJSON": "pm2 start pm2.json",
    "pm2:start": "pm2 start",
    "install:webpack": "npm install --save-dev webpack webpack-cli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scriptive/myordbok.git"
  },
  "author": "Khen Solomon Lethil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scriptive/myordbok/issues"
  },
  "homepage": "https://github.com/scriptive/myordbok#readme",
  "dependencies": {
    "@scriptive/evh": "file:../evh",
    "mysql": "^2.18.1",
    "mathjs": "^8.0.1",
    "myanmar-notation": "^1.4.0",
    "pluralize": "^8.0.0",
    "thesaurus": "0.0.1",
    "ttfinfo": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "axios": "^0.21.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
