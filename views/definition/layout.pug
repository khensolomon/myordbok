extends ../layout
block header
  include ../header

include utilities
block content
  div.ptl#content
    div
      //- +message(meta.msg)
      div.gads
        div(width='100vw' height='320' type='adsense' data-ad-client='ca-pub-2986689390924566' data-ad-slot='2945716327' data-auto-format='rspv' data-full-width='')
          div(overflow='')
      //- div.ads
      //-   div
      //-     p This property specifies whether to clip content or to add scrollbars when an element's content is too big to fit in a specified area.
      //-   div
      //-     p ads
      div.define(class=meta.type)
        case meta.type
          when 'pleaseenter'
            include ./pleaseenter
          when 'notfound'
            include ./notfound
          default
            +resultData(data)

block footer
  include ../footer