ul
  li.showMobileMenu
    nav-engine
    mixin li(v)
      li(class=v.route class={active: v.active || v.parent} class={hasChild: v.child})
        a(href=(v.url=='*' ? '/' : v.url.replace(/:\w+\?/g,'')))
          span #{v.text}
        if v.child
          ol.child
            each s, route in v.child
              +li(s)

    ol.menu.page
      each v, route in navPage
        +li(v)